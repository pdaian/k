      >
    <kompile-option name="-backend" value="java" />
    <all-programs>
      <krun-option name="-backend" value="java" />
      <krun-option name="--search" />
      <krun-option name="--pattern" value="&lt;solution&gt; Solution:K &lt;/solution&gt;" />
    </all-programs>
